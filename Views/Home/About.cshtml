    <style>
        .section-800x800 {
            padding: 0;
            background-color: #f8f9fa;
        }

        .card {
            border: none;
            background-color: transparent;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            margin: 0;
        }

            .card:hover .card-body {
                transform: scale(1.05);
                transition: transform 0.3s ease-in-out;
                box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
            }

        .card-body {
            transition: transform 0.3s ease-in-out, box-shadow 0.3s ease-in-out;
            position: relative;
            height: 200px;
            background-size: cover;
            background-position: center;
            border-radius: 0;
            overflow: hidden;
        }

        .overlay {
            position: absolute;
            bottom: 0;
            left: 0;
            right: 0;
            background-color: rgba(0, 0, 0, 0.6);
            color: #fff;
            padding: 10px;
        }

            .overlay h4 {
                margin-bottom: 5px;
            }

            .overlay p {
                font-size: 14px;
                margin-bottom: 0;
            }

        .rounded-text {
            background-color: #007bff;
            color: #fff;
            text-align: center;
            padding: 20px;
            height: 100%;
            border-bottom-right-radius: 200px;
        }

        .content {
            padding: 20px;
        }

            .content h3,
            .content h6 {
                text-align: center;
            }

            .content h3 {
                font-size: 28px;
                margin-bottom: 10px;
            }

            .content h6 {
                font-size: 16px;
                color: #6c757d;
                margin-bottom: 0;
            }

        .row.no-gutters {
            margin-right: 0;
            margin-left: 0;
        }

            .row.no-gutters > [class^="col-"] {
                padding-right: 0;
                padding-left: 0;
            }

        .row {
            margin-bottom: 0;
        }
    </style>
    <script>
        // Function to trigger the animation for each card
        function animateCards() {
            const cards = document.querySelectorAll('.card');

            cards.forEach((card, index) => {
                // Delay each card's animation by multiplying index with 150ms
                setTimeout(() => {
                    card.classList.add('fadeIn');
                }, index * 150);
            });
        }

        // Wait for the DOM content to be loaded
        document.addEventListener('DOMContentLoaded', () => {
            // Trigger the animation once the DOM content is loaded
            animateCards();
        });
    </script>
